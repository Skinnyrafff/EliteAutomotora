{
  "kind": "collectionType",
  "collectionName": "vehicles",
  "info": {
    "singularName": "vehicle",
    "pluralName": "vehicles",
    "displayName": "Vehicle"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    },
    "primaryPhoto": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "Photos": {
      "type": "media",
      "multiple": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "ownersCount": {
      "type": "integer",
      "min": 0
    },
    "location": {
      "type": "string"
    },
    "price": {
      "type": "integer"
    },
    "mileageKm": {
      "type": "integer"
    },
    "transmission": {
      "type": "enumeration",
      "enum": [
        "AT",
        "MT",
        "CVT",
        "DCT"
      ]
    },
    "year": {
      "type": "integer",
      "min": 1980,
      "max": 2025
    },
    "seller": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::seller.seller",
      "inversedBy": "vehicle"
    },
    "description": {
      "type": "blocks"
    },
    "estado": {
      "type": "enumeration",
      "enum": [
        "published",
        "reserved",
        "sold"
      ]
    }
  }
}
